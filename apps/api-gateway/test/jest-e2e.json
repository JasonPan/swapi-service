{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "roots": ["<rootDir>/apps/", "<rootDir>/lib/", "<rootDir>"],
  "modulePaths": ["<rootDir>"],
  "moduleNameMapper": {
    "^lib/common(|/.*)$": "<rootDir>/lib/common/$1"
  }
}
